@model SimpleWebPage.Models.VMR

@{
    ViewBag.Title = "Big Brother loves you";
}


<h2>@Model.name</h2>

<div class="row">
    <div class="col-md-6">
        <div class="well">
            <h5><b>Name:</b> @Model.name</h5>
            <h5><b>URI:</b> @Model.aliases[0].alias</h5>
            <h5><b>Meeting ID:</b> @Model.aliases[1].alias</h5>
            <h5><b>Host PIN:</b> @Model.pin</h5>
            <h5><b>Guest PIN:</b> @Model.guest_pin</h5>
        </div>
    </div>
</div>

